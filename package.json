{
  "name": "guro-and-the-muppets-music-generator",
  "version": "1.0.0",
  "description": "Swing jazz arrangement generator for Guro and the Muppets band",
  "scripts": {
    "generate-pdf": "bash scripts/generate-pdfs.sh",
    "pdf:test": "npx md-to-pdf songs/vaarherres-klinkekule/README.md",
    "build:site": "node src/scripts/build-site.js",
    "serve": "npx @11ty/eleventy --serve",
    "serve:only": "npx @11ty/eleventy --serve --input=site --output=docs",
    "deploy": "npm run build:site",
    "render:lilypond": "bash scripts/render-lilypond.sh"
  },
  "keywords": [
    "music",
    "swing",
    "jazz",
    "arrangement",
    "lindy-hop"
  ],
  "author": "Guro and the Muppets",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "archiver": "^7.0.1",
    "markdown-it": "^14.1.0",
    "md-to-pdf": "^5.2.5"
  }
}
